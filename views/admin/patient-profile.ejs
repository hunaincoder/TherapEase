<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>TherapEase</title>
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, user-scalable=0"
    />

    <link rel="shortcut icon" type="image/x-icon" href="/img/logo.png" />

    <link rel="stylesheet" href="/css/bootstrap.min.css" />

    <link rel="stylesheet" href="/css/feathericon.min.css" />

    <link rel="stylesheet" href="/plugins/morris/morris.css" />

    <link rel="stylesheet" href="/css/bootstrap.min.css" />

    <link
      rel="stylesheet"
      href="/plugins/fontawesome/css/fontawesome.min.css"
    />
    <link rel="stylesheet" href="/plugins/fontawesome/css/all.min.css" />

    <link rel="stylesheet" href="/plugins/fancybox/jquery.fancybox.min.css" />

    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/patient-style.css" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="assets/js/html5shiv.min.js"></script>
      <script src="assets/js/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!-- Main Wrapper -->
    <div class="main-wrapper">
      <!-- Header -->
            <div class="header">
			
				 <!-- Logo -->
				 <div class="header-left">
					<a href="/dashboard" class="logo">
					<img src="/img/logo.png" alt="Logo" width="140" height="50">
					</a>
					<a href="/dashboard" class="logo logo-small">
					<img src="/img/logo-small.png" alt="Logo" width="100" height="70">
					</a>
				</div>
			<!-- /Logo -->
				
				<a href="javascript:void(0);" id="toggle_btn">
					<i class="fe fe-text-align-left"></i>
				</a>
				
				<div class="top-nav-search">
					<form>
						<input type="text" class="form-control" placeholder="Search here">
						<button class="btn" type="submit"><i class="fa fa-search"></i></button>
					</form>
				</div>
				
				<!-- Mobile Menu Toggle -->
				<a class="mobile_btn" id="mobile_btn">
					<i class="fa fa-bars"></i>
				</a>
				<!-- /Mobile Menu Toggle -->
				
				<!-- Header Right Menu -->
				<ul class="nav user-menu">


					
					<!-- User Menu -->
					<li class="nav-item dropdown has-arrow">
						<a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
							<span class="user-img"><img class="rounded-circle" src="/img/profiles/avatar-01.jpg" width="31" alt="Ryan Taylor"></span>
						</a>
						<div class="dropdown-menu">
							<div class="user-header">
								<div class="avatar avatar-sm">
									<img src="/img/profiles/avatar-01.jpg" alt="User Image" class="avatar-img rounded-circle">
								</div>
								<div class="user-text">
									<h6><%= admin.username %></h6>
									<p class="text-muted mb-0">Administrator</p>
								</div>
							</div>
							<a class="dropdown-item" href="/dashboard">My Profile</a>
							<a class="dropdown-item" href="/logout">Logout</a>
						</div>
					</li>
					<!-- /User Menu -->
					
				</ul>
				<!-- /Header Right Menu -->
				
            </div>
			<!-- /Header -->
			
			 <!-- Sidebar -->
			 <div class="sidebar" id="sidebar">
				<div class="sidebar-inner slimscroll">
				  <div id="sidebar-menu" class="sidebar-menu">
					<ul>
					  <li >
						<a href="/dashboard"
						  ><i class="fe fe-home"></i> <span>Dashboard</span></a
						>
					  </li>
					  <li>
						<a href="/profile"
						  ><i class="fe fe-user"></i> <span>Profile</span></a
						>
					  </li>
		
					  <li>
						<a href="/appointment-list"
						  ><i class="fe fe-layout"></i> <span>Appointments</span></a
						>
					  </li>
					  <li>
						<a href="/therapist-list"
						  ><i class="fe fe-user"></i> <span>Therapists</span></a
						>
					  </li>
					  <li class="active">
						<a href="/patient-list"
						  ><i class="fe fe-user"></i> <span>Patients</span></a
						>
					  </li>
					  <li>
  <a href="/therapy-reports"><i class="fe fe-file"></i> <span>Therapy Reports</span></a>
</li>
					  <li>
						<a href="/transactions-list"
						  ><i class="fe fe-activity"></i> <span>Transactions</span></a
						>
					  </li>
					  <li>
						<a href="/Therapist-approval"><i class="fe fe-user-plus"></i> <span>Therapist Approval</span></a>
		
					  </li>
					  <li >
						<a href="/payment-approval"><i class="fe fe-file"></i> <span>Payment Approval</span></a>
		
					  </li>
					  <li>
						<a href="/refunds-approval"
						  ><i class="fe fe-file"></i> <span>refund Approval</span></a
						>
					  </li>
					</ul>
				  </div>
				</div>
			  </div>
			  <!-- /Sidebar -->

      <div class="page-wrapper">
        <div class="content">
          <div class="container">
            <% if (successMessage && successMessage.length > 0) { %>
            <div
              class="alert alert-success alert-dismissible fade show"
              role="alert"
            >
              <%= successMessage %>
              <button
                type="button"
                class="btn-close"
                data-bs-dismiss="alert"
                aria-label="Close"
              ></button>
            </div>
            <% } %> <% if (errorMessage && errorMessage.length > 0) { %>
            <div
              class="alert alert-danger alert-dismissible fade show"
              role="alert"
            >
              <%= errorMessage %>
              <button
                type="button"
                class="btn-close"
                data-bs-dismiss="alert"
                aria-label="Close"
              ></button>
            </div>
            <% } %>
            <div class="card">
              <div class="card-body">
                <div class="doctor-widget">
                  <div class="doc-info-left">
                    <div class="doc-info-cont">
                      <h4 class="doc-name"><%= patient.username %></h4>
                      <p class="doc-speciality">
                        <% if (patient.occupation) { %> <%= patient.occupation
                        %> <% } else { %> N/A <% } %>
                      </p>
                      <p class="doc-department">
                        <% if (patient.familyStructure) { %> <%=
                        patient.familyStructure %> <% } else { %> N/A <% } %>
                      </p>

                      <div class="clinic-details">
                        <p class="doc-location">
                          <i class="fas fa-map-marker-alt"></i> <%= patient.city
                          %>, <%= patient.country %>
                        </p>
                      </div>
                    </div>
                  </div>
                  <div class="doc-info-right">
                    <div class="clini-infos">
                      <ul>
                        <li>
                          <i class="fas fa-phone"></i> <% if (patient.mobile) {
                          %> <%= patient.mobile %> <% } else { %> N/A <% } %>
                        </li>
                        <li>
                          <i class="far fa-money-bill-alt"></i> Age: <%=
                          patient.age %>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <div class="card">
              <div class="card-body pt-0">
                <div class="tab-content pt-0">
                  <div
                    role="tabpanel"
                    id="doc_overview"
                    class="tab-pane fade show active"
                  >
                    <div class="row">
                      <div class="col-md-12 col-lg-9">
                        <div class="widget about-widget">
                          <h4 class="widget-title">About Me : </h4>
                          <p>
                            <%= patient.therapyProfile?.primary_concern || '' %>
                          </p>
                          <p><%= patient.therapyProfile?.impact || '' %></p>
                        </div>

                        <div class="widget education-widget">
                          <h4 class="widget-title">Family Details : </h4>
                          <ul class="experience-list">
                            <li>
                              <strong>Head of Family:</strong> <%=
                              patient.headOfFamily %>
                            </li>
                            <li>
                              <strong>Head of Family Contact:</strong> <%=
                              patient.headOfFamilyContact %>
                            </li>
                          </ul>
                        </div>

                        <div class="widget experience-widget">
                          <h4 class="widget-title">Therapy Profile : </h4>
                          <ul class="experience-list">
                            <li>
                              <strong>Past Experiences:</strong> <%=
                              patient.therapyProfile?.past_experiences || '' %>
                            </li>
                            <li>
                              <strong>Emotional State:</strong> <%=
                              patient.therapyProfile?.emotional_state || '' %>
                            </li>
                            <li>
                              <strong>Behavior Patterns:</strong> <%=
                              patient.therapyProfile?.behavior_patterns || '' %>
                            </li>
                            <li>
                              <strong>Therapy Goals:</strong> <%=
                              patient.therapyProfile?.therapy_goals || '' %>
                            </li>
                          </ul>
                        </div>

                        <div class="widget awards-widget">
                          <h4 class="widget-title">Contact Details : </h4>
                          <ul class="experience-list">
                            <li>
                              <strong>Phone:</strong> <%= patient.phone || 'N/A'
                              %>
                            </li>
                            <li>
                              <strong>Email:</strong> <%= patient.email %>
                            </li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /Page Content -->
      </div>
    </div>
    <!-- /Main Wrapper -->

    >

    <!-- jQuery -->
    <script src="/js/jquery-3.2.1.min.js"></script>

    <script src="/js/jquery.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>

    <!-- Fancybox JS -->
    <script src="/plugins/fancybox/jquery.fancybox.min.js"></script>

    <!-- Slimscroll JS -->
    <script src="/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <script src="/plugins/raphael/raphael.min.js"></script>
    <script src="/plugins/morris/morris.min.js"></script>
    <script src="/js/chart.morris.js"></script>

    <!-- Custom JS -->
    <script src="/js/script.js"></script>
  </body>

  <!-- doccure/doctor-profile.html  30 Nov 2019 04:12:16 GMT -->
</html>
